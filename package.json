{
  "name": "budgetplanner",
  "version": "1.0.0",
  "description": "Budget Planner App",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natttara/BudgetPlanner-WT.git"
  },
  "scripts": {
    "start": "parcel index.html",
  "build": "parcel build index.html --dist-dir docs --public-url ./",
    "build-pages": "parcel build --dist-dir docs --public-url /BudgetPlanner-WT/ src/*.html",
    "dev:build": "parcel build --dist-dir docs --no-content-hash assets/html/*.html",
    "dev:run": "npm run dev:build && npm run dev:start",
    "dev:start": "parcel serve --dist-dir docs assets/html/*.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d docs",
    "dev": "parcel serve index.html",
    "lint": "eslint ."
  },
  "dependencies": {
    "@google/generative-ai": "^0.22.0",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.1.0",
    "gh-pages": "^6.3.0",
    "loglevel": "^1.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "buffer": "^6.0.3",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "parcel": "^2.13.3",
    "process": "^0.11.10"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
